# Server Configuration
NODE_ENV=development
PORT=5001

# MongoDB Connection - Replace this placeholder with your actual MongoDB Atlas connection string
# Example: mongodb+srv://username:password@cluster0.mongodb.net/project_setu?retryWrites=true&w=majority
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster-url>/project_setu?retryWrites=true&w=majority

# JWT Configuration - CHANGE THESE IN PRODUCTION
JWT_SECRET=project_setu_dev_secret_key_change_in_production
JWT_EXPIRE=30d
JWT_COOKIE_EXPIRE=30

# File Upload Limits
MAX_FILE_UPLOAD=5

# Admin User (created on first startup)
ADMIN_NAME=Admin User
ADMIN_EMAIL=admin@projectsetu.org
ADMIN_PASSWORD=Admin@123ChangeThis 